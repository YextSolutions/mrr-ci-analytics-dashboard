{
    "$id": "mrr_reviewsByMonthSite",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Reviews by Month and Site",
    "description": "Reviews by Month and Site",
    "query": {
      "metrics": [
        "yext/NEW_REVIEWS"
      ],
      "dimensions": [
        "yext/MONTHS",
        "yext/PARTNERS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_12"
        },
        {
          "filter": "yext/partners",
          "in": [
            "559",
            "715,713,558",
            "439,588,792"
          ]
        }
      ]
    },
    "display": {
      "line": {
        "showLabels": true
      }
    }
  }