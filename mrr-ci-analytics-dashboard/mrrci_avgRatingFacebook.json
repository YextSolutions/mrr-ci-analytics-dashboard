{
    "$id": "mrrci_avgRatingFacebook",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Facebook Avg Rating",
    "description": "Rolling Average Rating for Facebook",
    "query": {
      "metrics": [
        "yext/COMPETITOR_REVIEW_RATING"
      ],
      "dimensions": [
        "yext/PARTNERS"
      ],
      "filters": [
        {
          "filter": "yext/competitorIntelligencePublisher",
          "in": [
            "559"
          ]
        }
      ]
    },
    "display": {
      "bar": {
        "showLabels": true
      }
    }
  }