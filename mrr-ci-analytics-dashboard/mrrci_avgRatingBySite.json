{
    "$id": "mrrci_avgRatingBySite",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Average Rating by Site",
    "description": "Average Rating by Site",
    "query": {
      "metrics": [
        "yext/AVERAGE_RATING"
      ],
      "dimensions": [
        "yext/PARTNERS"
      ],
      "filters": [
        {
          "filter": "yext/partners",
          "in": [
            "715,713,558",
            "439,588,792",
            "559"
          ]
        }
      ]
    },
    "display": {
      "bar": {
        "showLabels": true
      }
    }
  }
