{
    "$id": "mrrci_avgRatingFolderYelp",
    "$schema": "https://schema.yext.com/config/analytics/insight/v1",
    "name": "Average Rating by Folder (Yelp)",
    "description": "Average Rating by Folder",
    "query": {
      "metrics": [
        "yext/AVERAGE_RATING"
      ],
      "dimensions": [
        "yext/FOLDERS"
      ],
      "filters": [
        {
          "filter": "yext/date",
          "in": "MONTHS_12"
        },
        {
          "filter": "yext/partners",
          "in": [
            "439,588,792"
          ]
        }
      ]
    },
    "display": {
      "bar": {
        "horizontal": true,
        "showLabels": true
      }
    }
  }